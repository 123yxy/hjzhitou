@charset "UTF-8";
.demo-image{
    width: 100%;
}
.pagination,.pages1,.pages2,.pages3,.pages4{
	position: relative;
    text-align: center;
  	zoom: 1;
}
.pagination:before,.pagination:after,.pages1:before,.pages1:after ,.pages2:before,.pages2:after ,.pages3:before,.pages3:after,.pages4:before,.pages4:after{
    content:"";
    display:table;
}
.pagination:after,.pages1:after,.pages2:after,.pages3:after,.pages4:after{
	clear:both;
	overflow:hidden;
}
.pagination span,.pages1 span,.pages2 span,.pages3 span,.pages4 span{
	float: left;
	margin:0 5px;
	width: 38px;
    height: 38px;
    line-height: 38px;
    color: #bdbdbd;
    font-size: 14px;
}
.pagination .active,.pages1 .active,.pages2 .active,.pages3 .active,.pages4 .active{
	float: left;
	margin:0 5px;
	width: 38px;
    height: 38px;
    line-height: 38px;
    background: #e91e63;
    color: #fff;
    font-size: 14px;
    border: 1px solid #e91e63;
}
.pagination a,.pages1 a,.pages2 a,.pages3 a,.pages4 a{
	float: left;
	margin:0 5px;
	width: 38px;
    height: 38px;
    line-height: 38px;
    background: #fff;
    border: 1px solid #ebebeb;
    color: #bdbdbd;
    font-size: 14px;
}
.pagination a:hover,.pages1 a:hover,.pages2 a:hover,.pages3 a:hover,.pages4 a:hover{
	color:#fff;
    background: #e91e63;
}
.pagination .next,.pagination .prev,.pages1 .next,.pages1 .prev{
	font-family: "Simsun";
    font-size: 16px;
    font-weight: bold;
}
.now,.count{
	padding:0 5px;
	color:#f00;
}
.eg img{
	max-width: 800px;
	min-height: 500px;
}
input{
    float: left;
    margin:0 5px;
    width: 38px;
    height: 38px;
    line-height: 38px;
    text-align: center;
    background: #fff;
    border: 1px solid #ebebeb;
    outline: none;
    color: #bdbdbd;
    font-size: 14px;
}