.data-radio input,
.data-checkbox input {
	display: none;
}

.data-radio,
.data-checkbox {
	display: inline-block;
	margin-right: 15px;
}

.data-radio .radio {
	position: relative;
	display: inline-block;
	width: 13px;
	height: 13px;
	border: 1px solid #666;
	background: #fff;
	border-radius: 50%;
	cursor: pointer;
	margin: 0 5px;
	margin-left: 0;
}

.data-radio .radio.on:after {
	content: '';
	width: 9px;
	height: 9px;
	background: #5c5c5c;
	border-radius: 50%;
	position: absolute;
	left: 1px;
	top: 1px;
}

.library_ul .data-radio .radio.on:after {
	content: '';
	width: 9px;
	height: 9px;
	background: #36b8f4;
	border-radius: 50%;
	position: absolute;
	left: 2px;
	top: 2px;
}

.data-checkbox .checkbox {
	position: relative;
	display: inline-block;
	width: 12px;
	height: 12px;
	border: 1px solid #e4e4e;
	background: #fff;
	border-radius: 3px;
	cursor: pointer;
}

.data-checkbox .checkbox.on:after {
	content: '';
	width: 13px;
	height: 13px;
	background: url(../../images/checkboxOn.png) no-repeat center;
	position: absolute;
	left: 1px;
	top: 0;
}

.data-checkbox .checkboxWord,
.data-radio .radioWord {
	position: relative;
	top: -2px;
	cursor: pointer;
}
