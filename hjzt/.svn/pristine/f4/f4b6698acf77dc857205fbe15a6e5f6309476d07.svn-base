#scrollbar1,
#scrollbar2,
#scrollbar3,
#scrollbar4,
#scrollbar5,
#scrollbar6 {
	width: 215px;
	clear: both;
	margin: 0 auto;
}

.viewport {
	width: 185px;
	left: 8px;
	height: 400px;
	overflow: hidden;
	position: relative;
}

.overview {
	list-style: none;
	position: absolute;
	left: 0;
	top: 0;
}

.scrollbar,
.thumb .end,
.thumb {
	background: transparent url(../images/bg-scrollbar.png) no-repeat;
}

.scrollbar {
	position: relative;
	background-position: 0 0;
	float: right;
	width: 15px;
}

.track {
	background: transparent url(../images/bg-scrollbar-end.png) no-repeat 0 100%;
	height: 100%;
	width: 13px;
	position: relative;
	padding: 0 1px;
}

.thumb {
	background-position: 100% 100%;
	height: 20px;
	width: 13px;
	cursor: pointer;
	overflow: hidden;
	position: absolute;
	top: 0;
}

.thumb .end {
	background-position: 100% 0;
	height: 10px;
	width: 13px;
}


