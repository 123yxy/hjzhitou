.right_main.library {
	padding-left: 0;
	padding-top: 0;
	padding-bottom: 100px;
}

.library_tab {
	width: 100%;
	margin: 0 auto;
	border-bottom: 1px solid #e4e4e4;
	position: relative;
	z-index: 1;
}

.library_tab ul li {
	cursor: pointer;
	display: inline-block;
	height: 42px;
	line-height: 42px;
	text-align: center;
	padding: 0 30px;
	box-sizing: border-box;
}

.library_tab ul li.on {
	background: #fff;
	border: 1px solid #e4e4e4;
	border-bottom: none;
	position: relative;
	bottom: -2px;
	z-index: 2;
}

.library_change {
	width: 100%;
	margin: 0 auto;
	background: #fff;
	border: 1px solid #e4e4e4;
	border-top: 0;
	box-sizing: border-box;
	padding: 30px;
}

.library_change {
	width: 100%;
	margin: 0 auto;
}

.change_yx {
	width: 100%;
	margin: 0 auto;
}

.change_yx span {
	display: block;
	width: 4%;
	min-width: 82px;
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	color: #999;
	float: left;
}

.change_yx .chenge_list {
	width: 93%;
	float: left;
}

.change_yx .chenge_list a {
	display: inline-block;
	margin-right: 10px;
	margin-bottom: 10px;
	font-size: 14px;
	color: #999;
	padding: 3px 20px 3px 10px;
	position: relative;
	border: 1px solid #e4e4e4;
	border-radius: 3px;
}

.change_yx .chenge_list a i {
	width: 12px;
	height: 12px;
	position: absolute;
	top: 7px;
	right: 5px;
	background: url(../images/sanban_shanchu.png) center center no-repeat;
	background-size: 12px 30px;
	background-position: 1px 1px;
}

.change_yx .chenge_list a:hover i {
	background-position: 1px -17px;
}

.change_dblist {
	width: 100%;
	margin: 0 auto;
}

.change_xlist {
	width: 206px;
	box-sizing: border-box;
	float: left;
	border: 1px solid #e4e4e4;
}

.library_seach {
	width: 185px;
	box-sizing: border-box;
	padding: 10px 15px 10px 15px;
}

.library_seach input {
	display: block;
	width: 100%;
	height: 30px;
	box-sizing: border-box;
	padding-left: 10px;
	font-size: 14px;
	color: #666;
	line-height: 30px;
	background: url(../images/search.png) right center no-repeat;
	border: 1px solid #e4e4e4;
}

.library_ul {
	width: 185px;
}

.library_ul ul li {
	width: 100%;
	cursor: pointer;
	color: #666;
	font-size: 14px;
	height: 30px;
	line-height: 30px;
	overflow: hidden;
	box-sizing: border-box;
	padding-left: 14px;
	box-sizing: border-box;
}

.library_ul ul li.on {
	color: #36b8f4;
	background: #fafafa;
}


