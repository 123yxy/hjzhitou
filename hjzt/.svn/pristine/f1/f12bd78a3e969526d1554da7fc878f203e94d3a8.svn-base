/*表格样式*/
.caiwu-table .table{
	margin-bottom: 0;
}
.caiwu-table .table thead tr{
	background-color: #559cd9;
	color: #fff;
	font-size: 14px;
}
.caiwu-table .table td{
	font-size: 14px;
	color: #333;	
}
.caiwu-table .table td.shuzi{font-family: arial;}
.caiwu-table .table tr td,.caiwu-table .table tr th{border-top: 0;border-bottom: 0;padding:0px;height: 30px;line-height: 30px;}
/*.caiwu-table .table tr td,.caiwu-table .table tr th{width: 30%;}*/
.caiwu-table .table tr td:nth-child(1),.caiwu-table .table tr th:nth-child(1){padding-left: 20px;box-sizing: border-box;width: 30%;}
.caiwu-table .table tr:nth-child(even){background-color: #f5f7f9;}


