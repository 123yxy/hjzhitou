       <style type="text/css">
			.login_header {
				    width: 100%;
				    background: #2e78c2;
				}
				.newlogo1{
   					 overflow: hidden;
    				margin-top: 16px;
				}
				
				.login_header {
				    width: 100%;
				    height: 65px;
				    line-height: 65px;
				}
				.login_header {
				    width: 100%;
				    margin: 0 auto;
				   padding-top: 0px;
				}


       </style>
