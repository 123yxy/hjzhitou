/*相关图表样式*/
.tb-mask-box{
	position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
    z-index: 1000;
}
.tb-mask{
    position: fixed;
    top: 0;
    left: 0;
    bottom:0;
    right:0;
    margin:auto;
    width:1200px;
    height: 580px;
    border-radius: 5px;
    border: 1px solid #e4e4e4;
    background: #fff;
    box-sizing: border-box;
    box-shadow: 0 0 8px rgba(25,133,226,.35);
}
.tb-mask-left{
	width:20%;
	float:left;
	height:100%;
	box-sizing: border-box;
	padding:0 20px;
	overflow-y: auto;
	    box-shadow: 1px 0px 2px 0px #dbecfa;
    border-right: 1px solid #e4e4e4;
}
.content-box{
	margin-top:20px;
	width:110%;
}
.tb-mask-right{
	width:80%;
	float:left;
	height:100%;
	box-sizing: border-box;
	padding:0 20px;
	overflow-y: auto;
}
.tb-mask .mask-title{
    height:46px;
    width:100%;
    border-bottom: 1px solid #e4e4e4;
}
.mask-title>span:first-child{
    font-size:16px;
    color:#333;
    font-weight: bold;
    line-height:16px;
    margin-top:20px;
}
.mask-title button{
    width:100px;
    height:28px;
    margin-top:9px;
    margin-right:45px;
}
.mask-title .close{
    display: inline-block;
    width:14px;
    height:14px;
    background:url("../../../saasBeta/images/Image/close_mask.png");
    position:absolute;
    top:16px;
    right:20px;
    cursor: pointer;
}
.mask-title2{
    height:28px;
    line-height:28px;
    margin-top:20px;
    position:relative;
}
.y-box{
    height:28px;
    line-height:28px;
    display: inline-block;
    margin-right:0px;
}
.y-box>span{
    color:#666;
    float:left;
    padding:0 9px;
    cursor:pointer;
}
