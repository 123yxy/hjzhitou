.newlogo{ width:12.5%; height: 54px; overflow: hidden;}

.newlogo a{display: block;width:153px;}
.newlogo img{ display: block; max-width: 127px; margin: 0 auto;}

.newheaders{ padding-left: 0;}
.footer2 {
	width: 100%;
	height: 47px;
	line-height: 47px;
	position: fixed;
	bottom: 0;
	z-index: 3;
	right: 0;
	box-sizing: border-box;
	transition: all 0.5s ease-in-out;
	background: #5b6e84;
}
.footer2 p{ display: block;width: 100%; line-height: 47px; font-size: 12px; color: #ccc; text-align: left; box-sizing: border-box; padding-left: 30px;}
.footer2 a{position: fixed; right: 10px; bottom: 8px; z-index: 4; display: block; width: 30px; height: 30px; top: 8;}
.footer2 a img{display: block;width: 30px; height: 30px; margin: 0 auto;}
.main_project{ width: 100%; margin: 0 auto; box-sizing: border-box; padding: 70px 30px; background: #fafafa; }
.project_info{ width: 100%; max-width: 640px;margin: 0 auto; padding: 0 10px;}
.pro_title{ width: 100%; margin: 0 auto; margin-top: 59px; border-bottom: 1px solid #ccc; padding-bottom: 20px;}
.pro_top{ width: 75%; margin: 0 auto; text-align: center;}
.pro_logo{ width: 50%; box-sizing: border-box; padding-top: 5px;}
.pro_logo img{ display: block; width: 180px;height:43px; float: right;}
.pro_tit_info{width:50%;box-sizing: border-box; padding-left: 10px;}
.pro_tit_info p{ display: block; width: 100%; font-size: 20px; color: #666;text-align: left;}
.pro_tit_info p i{ font-weight: bold; display: inline-block;font-size: 20px; font-style: normal;}
.pro_tit_info span{ display: block; width: 100%; font-size: 14px; color: #666; text-align: left;}
.pro_img{ width: 100%; margin: 0 auto; margin-top: 20px;}
.pro_img img{ display: block; width: 100%; margin: 0 auto;}
.pro_info_dbox{ width: 100%; border: 1px solid #ddd; margin-top: 45px; position: relative; z-index: 1; padding-bottom: 22px;box-sizing: border-box;}
.fabu{width: 100%;margin-top: 20px;height: 36px;line-height: 36px;text-align: center;font-size: 18px;color: #fff;background-color: #5bc0de;cursor: pointer;}
.pri_title{ position: absolute; width: 40%; left: 50%; margin-left:-20%; background: #fafafa; top: -21px; z-index: 2;}
.pri_title h2{ font-size: 24px; display: block; width: 100%; color: #707070; text-align: center;}
.pri_title span{ display: block;width: 100%; color: #999; font-size: 14px; text-align: center;}
.pri_info{ width: 100%; margin: 0 auto; margin-top: 75px; padding: 0 20px;box-sizing: border-box;}
.pri_l{width: 15%; box-sizing: border-box;}
.pri_l span{ display: block; width: 100%; text-align: right; font-size: 14px; color: #848484;line-height: 30px;}
.pri_r{width: 85%; box-sizing: border-box;}
.pri_r p{display: block; width: 100%; font-size: 14px; color: #333; line-height: 30px;}
/*鑸嗘儏鎬昏-鑸嗘儏璇︽儏*/
.yqdetail_title{ width: 100%; margin: 0 auto; margin-top: 20px; height: 40px;}
.yqdetail_title h2{display: inline-block; font-size: 16px; color: #666; line-height: 40px;}
.yq_keysword_tab{ height: 40px; overflow: hidden;}
.kesbox{height: 40px; overflow: hidden; text-align: left;}
.kesbox span{display: inline-block; color: #666; font-size: 14px; float: left; height: 40px; line-height: 40px; margin-left: 50px; margin-right: 20px;}
.kesbox a{ display: inline-block; box-sizing: border-box; color: #fff; position: relative; float: left; height: 22px; padding: 0 10px; padding-right: 25px; margin-top: 9px; margin-right: 5px; border-radius: 5px;}
.kesbox a:nth-child(2){ background: #CB93DD;}
.kesbox a:nth-child(3){ background: #89c4f4;}
.kesbox a:nth-child(4){ background: #ecbc29;}
.kesbox a:nth-child(5){ background: #f3565d;}
.kesbox a:nth-child(6){ background: #428bca;}
.kesbox a i{ display: block; width: 10px; height: 10px; position: absolute; right: 5px; top: 5px; background: url(../images/keyscha.png) center center no-repeat; cursor: pointer;}
.keyscz span{ display: block; float: left; width: 20px; height: 20px; margin-top: 10px; cursor: pointer; box-sizing: border-box; background: url(../images/yqkeysadd.png) center center no-repeat;}
.keyscz span.on{ background: url(../images/yqkeysaddhover.png) center center no-repeat;}
.keyscz input{display: none; width: 110px; height: 25px; margin-top: 8px; margin-left: 15px; box-sizing: border-box; border: 1px solid #e1e1e1; border-radius: 5px; padding-left: 10px; color: #666; font-size: 14px; line-height: 25px; float: left;}
.keyscz i{ display: none; width: 20px; height: 20px; cursor: pointer; margin-top: 10px; box-sizing: border-box; float: left; margin-left: 10px;background: url(../images/truefalse.png) center center no-repeat;}
.keyscz i.keys_qd{background-position: 0px 4px;}
.keyscz i.keys_qd:hover{background-position: -112px 4px;}
.keyscz i.keys_qx{background-position: -28px 4px;}
.keyscz i.keys_qx:hover{background-position: -140px 4px;}
.yq_keys_r{ width: 212px; height: 28px; background: #fff; line-height: 28px; border: 1px solid #e4e4e4; position: relative;}
.yq_keys_r .selectBox{top: 0;}
.yq_keys_r .selectBox p{color:#999;height: 30px;line-height: 30px;}
.yq_keys_r .selectBox p:after{background-position: -93px 2px;}
.yq_keys_r ul li{text-align: left;padding-left:15px;box-sizing: border-box;}
.yq_zl_tubiao{ width: 100%; border: 1px solid #e4e4e4;}
.yq_zl_nav{width: 100%; margin: 0 auto; height: 35px; box-sizing: border-box; padding: 20px 0 0 65px;}
.yq_zl_nav a{ display: inline-block; padding: 0 10px; font-size: 14px; color: #666; border-radius: 5px; margin-right: 10px;}
.yq_zl_nav a.on{ background: #5bc0de; color: #fff;}
.yq_zl_tu{ width: 100%; height: 400px;}
.yq_new_lie{ width: 100%; margin: 0 auto; margin-top: 35px;}
.yq_new_title{width: 100%; margin: 0 auto; border-bottom: 1px solid #e4e4e4; height: 45px; line-height: 45px;}
.yq_new_title span{ display: block; width: 6%; float: left; font-size: 16px; color: #666; text-align: center;}
.yq_new_title a{ display: block; width: 46%; float: left;font-size: 16px; color: #666;text-align: center;}
.yq_new_title i{ display: block; width: 20%; float: left;font-size: 16px; color: #666;text-align: center;}
.yq_new_lie ul li{ width: 100%; margin: 0 auto; height: 45px; border-bottom: 1px solid #e4e4e4; line-height: 45px;}
.yq_new_lie ul li:after{clear: both;}
.yq_new_lie ul li span{display: block; width: 6%;float: left; font-size: 14px; color: #666; text-align: center;}
.yq_new_lie ul li span em{ display: inline-block; font-style: normal; font-size: 14px; color: #fff; padding: 0px 5px 2px 5px; box-sizing: border-box; border-radius: 5px; margin-top: 11px;}
.yq_new_lie ul li.dwfm span em{ background: #37B7F3;}
.yq_new_lie ul li.jysj span em{ background: #f3565d;}
.yq_new_lie ul li.tfsj span em{ background: #89c4f4;}
.yq_new_lie ul li.ywsj span em{ background: #ecbc29;}
.yq_new_lie ul li.hysj span em{ background: #428bca;}
.yq_new_lie ul li.rwfm span em{ background: #37B7F3;}
.yq_new_lie ul li a{display: block; width: 46%;float: left; font-size: 14px; color: #666; text-align: center;}
.yq_new_lie ul li a:hover{ color: #3a80a3;}
.yq_new_lie ul li i{display: block; width: 20%;float: left; font-size: 14px; color: #666; text-align: center; line-height: 45px;color: #999;}
/*閫傞厤鎵嬫満 star*/
 @media only screen and (min-width: 100px) and (max-width: 640px) {
.newlogo{ width:20%;}
.project_info{ width: 100%; max-width: 95%;}

}
 @media only screen and (min-width: 100px) and (max-width: 1600px) {
.yq_new_title span{width: 14%;}
.yq_new_lie ul li span{width: 14%;}

}
/*閫傞厤灏忓睆骞� end*/
