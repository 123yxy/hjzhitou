		$("#changeValue").change(function(){
			var flag=0;
		   	$.each(blockDiv,function(key,value){
		   		if(!value){
		   			flag++;
		   		}
		   	})
		   	var content=getUrlParam("content");

		   	if(flag==12){
				$("#none_datas").show();
				$("#no_data_titles").text(content);
				$("#data_name").text(content);
//				$("#tab_data").hide();
//				$("#tab_yb").hide();
//				$("#tab_xw").hide();
//				$("#tab_gg").hide();
//				$("#tab_gx").hide();
			}else{
				$("#none_datas").hide();
			}
			
		})
		function changeTime(){
			var countTime=0;
			var totalcount=0;
			$(".search_tab li").each(function(index,item){
				if(($(item).attr("data-time")!=null || $(item).attr("data-time")!=undefined) && $(item).attr("data-count")!=null || ($(item).attr("data-count")!=undefined)){
					countTime+=Number($(item).attr("data-time"));
					totalcount+=Number($(item).attr("data-count"));
				}
			})
			$("#totalTime").text(countTime);
			$("#totalCount").text(totalcount);
		}
//		$("#changeValue").change();
//		$("#tab_gg").click(function(){
//		    $(".select-box").removeClass("hide");
//		})
		$(".search_tab li").click(function(){
		    if($(this).attr("name")=="公告"){
                $(".select-box").removeClass("hide");
                $(".search_nav").css("height","110px")
			}else if($(this).attr("name")=="znyb"){
                $("#content_znyb").show()
			}else{
                $(".select-box").addClass("hide");
                $(".search_nav").attr("style","height:56px;")
			}

		})
		//图标点击进入定位
		var tab=getUrlParam("tab");
		if(tab!='' && tab!=null && tab!=undefined){
			console.log(tab!='' && tab!=null && tab!=undefined)
			var liOn=tab.split("_")[1];
			$(".serach_info_box").hide();
			if(tab!="content_all"){
				$("#content_all").css("display","none");
			}
			$("#"+tab).show();
			$(".search_tab li").removeClass("on");
			$("#tab_"+liOn).addClass("on");
		}
		
	</script>
