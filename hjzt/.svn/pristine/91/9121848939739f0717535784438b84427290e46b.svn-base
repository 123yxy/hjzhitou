    <body>
        <!--header  star-->
        <!--侧导航 star-->

        <!--侧导航 end-->
        <!--主体 star-->
        <div class="right_main">

            <!--main  star-->
            <div class="main">
                <div class="main_box">
                    <div class="map_title">
                        <span class="fl">企业地图</span>
                        <div class="map_nav fl">
                            <ul>
                                <li class="hover"><i name="showTime">天</i>
                                    <ul>
                                        <li name="time" data-value="D">今天</li>
                                        <li name="time" data-value="LD">昨天</li>
                                    </ul>
                                </li>
                                <li><i name="showTime">月</i>
                                    <ul>
                                        <li name="time" data-value="LM">近一月</li>
                                        <li name="time" data-value="L2M">近两月</li>
                                        <li name="time" data-value="L3M">近三月</li>
                                    </ul>
                                </li>
                                <li><i name="showTime">年</i>
                                    <ul>
                                        <li name="time" data-value="LY">近一年</li>
                                        <li name="time" data-value="L2Y">近两年</li>
                                        <li name="time" data-value="L3Y">近三年</li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="clr"></div>
                    </div>
                    <div class="clr"></div>
                    <div class="company_map">
                        <div class="company_map_left fl">
                            <div class="map_box" >

                                <div id="map"></div>
                            </div>

                        </div>	
                        <div class="company_map_right fr">
                            <div class="map_rt">
                                <div class="r_select">
                                    <span class="fl">全部行业</span>
                                    <div class="r_industry_sele">
                                        <div class="r_indu_title">
                                            <p><input type="checkbox" id="checkAll"/>全部行业</p>
                                            <b>已选择<em id="che_yx">(0)</em></b>
                                        </div>
                                        <div id="tradeList" class="r_indu_box">
                                           
                                        </div>
                                        <div class="r_indu_btn">
                                            <a href="javascript:;" class="true" onclick="slideUp('1', this);">确定</a><a onclick="slideUp('2', this);" href="javascript:;" class="qx_bt">取消</a>
                                            <div class="clr"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="clr"></div>
                            </div>
                            <div class="map_r_box">
                                <a href="companyMap.html"  >公司</a><a href="companyQuotation.html" class="on">行情</a><a href="companyTransaction.html">交易</a>
                                <div class="clr"></div>
                            </div>
                            <div class="clr"></div>
                            <div class="map_t_box">
                                <div class="gongs_hq map_title_wz">
                                    <table width="100%" border="1">
                                        <thead>
                                            <tr>
                                                <th scope="col">排名</th>
                                                <th scope="col" class="gs"><span>区域</span></th>
                                                <th scope="col" class="zxj"><span>涨幅</span></th>
                                                <th scope="col" class="junjia"><span>均价</span></th>
                                                <th scope="col" class="qyzs"><span>企业总数</span></th>
                                            </tr>

                                        </thead>
                                    </table>
                                </div>
                                <div class="map_r_info gongs_hq">

                                    <table width="100%" border="1">
                                        <thead>
                                            <tr>
                                                <th scope="col">排名</th>
                                                <th scope="col" class="gs"><span>区域</span></th>
                                                <th scope="col" class="zxj"><span>涨幅</span></th>
                                                <th scope="col" class="junjia"><span>均价</span></th>
                                                <th scope="col" class="qyzs"><span>企业总数</span></th>
                                            </tr>

                                        </thead>
                                        <tbody id="mtList" > 
                                        </tbody>
                                        <tr>
                                            <td colspan="5"><a href="javascript:;" id="msg" onclick="loadPage(this);">点击查看更多</a></td>
                                        </tr>
                                    </table>


                                </div>
                            </div>
                        </div>
                        <div class="clr"></div>
                    </div>

                </div>
