.backbj {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: #000;
	opacity: .6;
	z-index: 1002;
}
.setTix {
	width: 498px;
	z-index: 1008;
	display: none;
	background: #fff;
	border-radius: 3px;
	border: 1px solid #f3f3f3;
	position: fixed;
	top: 12%;
	left: 50%;
	margin-left: -400px;
	padding-bottom: 30px;
	box-sizing: border-box;
}

.setTix_title {
	width: 100%;
	margin: 0 auto;
	margin-top: 25px;
	padding:0px 20px;
	border-bottom: 1px solid #e4e4e4;
	padding-bottom: 25px;
	box-sizing: border-box;
}

.setTix_title h2 {
	display: block;
	width: 100%;
	text-align: center;
	font-size: 16px;
	color: #333;
}

.setTix_title span {
	display: block;
	width: 100%;
	text-align: center;
	font-size: 14px;
	color: #666;
}

.setTix_info {
	width: 93%;
	margin: 0 auto;
}

.setTix_info ul li {
	width: 100%;
}

.set_tix_box {
	width: 100%;
	margin: 0 auto;
	background: #fafafa;
	margin-top: 10px;
	box-sizing: border-box;
	padding: 0 0px 0px 15px;
}

.set_tix_box span {
	display: block;
	width:90px;
	height: 40px;
	line-height: 40px;
	font-size: 14px;
	color: #333;
	padding-left: 18px;
	box-sizing: border-box;
	position: relative;
}
.set_tix_box span.on{
	color: #36b8f4;
}
.set_tix_box .gjtx_xk,.set_tix_box .ggtx_xk{
	margin-top: 9px;
}
.set_tix_box span:after{
	content: " ";
	display: block;
	width: 8px;
	height: 8px;
	border-radius: 4px;
	background-color: #36b8f4;
	position: absolute;
	top:16px;
	left: 0px;
}
.setTixCaozuo {
	width: 170px;
	height: 40px;
	line-height: 40px;
}

.set_check {
	width: 100px;
	height: 40px;
	line-height: 40px;
	box-sizing: border-box;
	padding-top: 2px;
}

.set_zk {
	width: 60px;
	cursor: pointer;
}

.set_zk i {
	display: block;
	width: 100%;
	height: 40px;
	line-height: 40px;
	background: url(../images/tx_sq.png) left center no-repeat;
	padding-left: 16px;
	box-sizing: border-box;
	font-size: 14px;
	color: #666;
}

.set_zk i:hover {
	background: url(../images/tx_sq_hover.png)left center no-repeat;
	color: #36b8f4;
}

.set_zk i.on {
	background: url(../images/tx_zk.png) left center no-repeat;
}

.set_zk i.on:hover {
	background: url(../images/tx_zk_hover.png)left center no-repeat;
}

.setTix_info ul li a {
	display: inline-block;
	margin-top: 40px;
	box-sizing: border-box;
	float: left;
	width: 160px;
	height: 40px;
	border-radius: 3px;
	line-height: 40px;
	text-align: center;
	font-size: 16px;
}

.setTix_info ul li a.baocun {
	background: #36b8f4;
	color: #fff;
	border: 1px solid #36b8f4;
	margin-right: 20px;
	margin-left: 60px;
}

.setTix_info ul li a.baocun:hover {
	background-color: #1aa6e6;
}

.setTix_info ul li a.qxb {
	background: #fff;
	color: #36b8f4;
	border: 1px solid #36b8f4;
}

.setTix_info ul li a.qxb:hover {
	background-color: #36b8f4;
	color: #fff;
}

.set_down {
	width: 100%;
	margin: 0 auto;
	padding-left: 10px;
	box-sizing: border-box;
}

.set_dow_title {
	width: 100%;
	margin: 0 auto;
}

.set_dow_title p {
	margin-top: 3px;
	display: block;
	width: 100%;
	margin: 0 auto;
	font-size: 14px;
	color: #333;
	height: 48px;
	line-height: 48px;
}

.set_dow_title p em {
	display: inline-block;
	color: #f3565d;
	font-size: 16px;
}

.set_dow_input {
	width: 100%;
	margin: 0 auto;
}

.set_dow_input input {
	display: block;
	width: 180px;
	border-radius: 3px;
	border: 1px solid #e4e4e4;
	height: 36px;
	line-height: 36px;
	box-sizing: border-box;
	padding-left: 14px;
	font-size: 14px;
	color: #666;
	background: #fff;
	float: left;
}
.set_dow_input input:hover{
	border: 1px solid #36b8f4;
}
.set_dow_input input:placeholder{
	color: #999;
}
.set_dow_input em {
	display: inline-block;
	font-style: normal;
	font-size: 14px;
	color: #666;
	height: 36px;
	line-height: 36px;
	float: left;
}
.set_dow_input em.mubiao_yuan{
	margin-right: 20px;
}
.set_dow_textar {
	width: 100%;
	margin: 0 auto;
	margin-top: 14px;
	padding-bottom: 10px;
	box-sizing: border-box;
}

.set_dow_textar b {
	display: block;
	width: 100%;
	margin: 0 auto;
	font-style: normal;
	font-size: 14px;
	color: #333;
}
.setTix .n_gs_close span{
	right: 14px;
}
.set_dow_textar textarea {
	display: block;
	width: 100%;
	height: 58px;
	margin: 0 auto;
	margin-top: 8px;
	border: 1px solid #e4e4e4;
	border-radius: 3px;
	box-sizing: border-box;
	padding:8px 14px;
	font-size: 14px;
	color: #666;
}

.n_gs_close {
	width: 100%;
	height: 41px;
	background-color: #36b8f4;
	border:1px solid #36b8f4;
	position: relative;
	/*margin-top: -1px;
	margin-left: -1px;*/
	border-radius: 3px 3px 0px 0px;
}

.n_gs_close span {
	display: block;
	cursor: pointer;
	width: 20px;
	height: 20px;
	position: absolute;
	top: 12px;
	right: 30px;
	background: url(../images/czzx_tc_cha.png) center center no-repeat;
	background-size: 20px 20px;
}
.n_gs_close h2.gjgs_xinxi{
	display: inline-block;
	width: 100%;
	height: 42px;
	text-align: center;
	line-height: 42px;
	font-size: 16px;
	color: #fff;
}
.n_gs_title {
	width: 100%;
	margin: 0 auto;
	margin-top: 0px;
	height: 45px;
	background: #f0f6fa;
}

.n_gs_title span {
	display: block;
	width: 100%;
	height: 45px;
	line-height: 45px;
	text-align: center;
	font-size: 18px;
	color: #333;
}

.n_gs_dbox {
	width: 100%;
	height: 505px;
	border: 1px solid #e5eff6;
	box-sizing: border-box;
	margin: 0 auto;
	overflow: auto;
}

.n_gs_close b {
	display: inline-block;
	width: 100%;
	text-align: center;
	line-height: 42px;
	font-size: 16px;
	color: #fff;
}

.n_gs_dbox ul li {
	width: 50%;
	box-sizing: border-box;
	float: left;
	border-bottom: 1px solid #e5eff6;
}

.n_gs_dbox ul li span {
	display: block;
	float: left;
	width: 30%;
	max-width: 140px;
	font-size: 14px;
	color: #999;
	text-align: center;
	line-height: 38px;
}

.n_gs_dbox ul li p {
	display: block;
	width: 70%;
	overflow: hidden;
	float: left;
	box-sizing: border-box;
	border-left: 1px solid #e5eff6;
	border-right: 1px solid #e5eff6;
	line-height: 30px;
	color: #666;
	padding: 4px 30px;
	text-align: left;
}
.setTix_info ul li a {
	display: inline-block;
	margin-top: 40px;
	box-sizing: border-box;
	float: left;
	width: 160px;
	height: 40px;
	border-radius: 3px;
	line-height: 40px;
	text-align: center;
	font-size: 16px;
}

.setTix_info ul li a.baocun {
	background: #36b8f4;
	color: #fff;
	border: 1px solid #36b8f4;
	margin-right: 20px;
	margin-left: 60px;
}

.setTix_info ul li a.baocun:hover {
	background-color: #1aa6e6;
}

.setTix_info ul li a.qxb {
	background: #fff;
	color: #36b8f4;
	border: 1px solid #36b8f4;
}

.setTix_info ul li a.qxb:hover {
	background-color: #36b8f4;
	color: #fff;
}
.data-checkbox .checkbox {
     width: 12px; 
     height: 12px; 
    border: 1px solid #e4e4e4;
   }
.setTix_info .data-checkbox .checkbox.on:after{
	left: -1px;
	top:-1px;
	background: url(../images/gouxuan.png)center center no-repeat;
}
/*操作中心里的删除提示信息*/
.tub_tc_publick{
	width: 100%;
	height: 100%;
	position: fixed;
	top:0px;
	left: 0px;
	z-index: 1012;
	background: rgba(0,0,0,.5);
}
.tips_shanchu_publick{
	width: 442px;
	height: 290px;
	background-color: #fff;
	position:fixed;
	z-index: 1013;
	top:50%;
	left: 50%;
	margin-top: -145px;
	margin-left: -221px;
	padding:20px;
	box-sizing: border-box;
}
.tips_shanchu_publick h2{
	width: 100%;
	font-size: 16px;
	color: #333;
	text-align: center;
	padding-bottom: 14px;
	border-bottom:1px solid #e4e4e4;
	position:relative;
}
.tips_shanchu_publick h2:after{
	display: block;
	content:'';
	width: 24px;
	height: 24px;
	position: absolute;
	top:0px;
	left: 130px;
	background:url(../images/sc_tips.png)center center no-repeat;
