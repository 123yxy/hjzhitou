.tips_shanchu_publick p{
	width: 100%;
	line-height:100px;
	font-size: 14px;
	color: #666;
	text-align: center;
}
.shanchu_btn2{
	width: 100%;
	margin-top: 20px;
	text-align: center;
}
.shanchu_btn2 span{
	display: inline-block;
	width: 120px;
	height: 36px;
	line-height:36px;
	font-size: 14px;
	text-align: center;
	border-radius: 5px;
	cursor:pointer;
}
.shanchu_btn2 span.shanc_btn2{
	color: #fff;
	background-color: #36b8f4;
	margin-right: 20px;
}
.shanchu_btn2 span.shanc_qux2{
	color: #36b8f4;
	border:1px solid #36b8f4;
	box-sizing: border-box;
}