.bottom_mark {
	-webkit-animation: 1.5s ease 0s normal none infinite running sdb05;
	 animation: 1.5s ease 0s normal none infinite running sdb05;
    position: relative;
    top: 0;
}


@keyframes sdb05 {
0% {
    opacity: 0.3;
    transform:translate(0px, 0px);
}
50% {
    opacity: 1;
}
100% {
    opacity: 0.3;
    transform:translate(0px, 20px);
}
}
@keyframes sdb05 {
0% {
    opacity: 0.3;
    transform:translate(0px, 0px);
}
50% {
    opacity: 1;
}
100% {
    opacity: 0.3;
    transform:translate(0px, 20px);
}
}
