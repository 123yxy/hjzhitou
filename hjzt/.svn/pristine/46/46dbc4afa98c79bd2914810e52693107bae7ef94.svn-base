/**
 *popUP
 */
.wj_clear { clear:both; overflow:hidden; zoom:1;}
.wj_mark,.wj_data_mark { display:none; position:fixed; top:0; left:0; background:rgba(24,25,50,.9); width:100%; height:100%; z-index:1116;}
.wj_popUP,.wj_data_popUP { display:none; position:fixed; top:50%; left:50%; transform:translate(-50%,-50%); -ms-transform:translate(-50%,-50%); -moz-transform:translate(-50%,-50%); -webkit-transform:translate(-50%,-50%); -o-transform:translate(-50%,-50%); z-index:1117;}
.wj_popUP_wrap { background:#fff; border-radius:5px; padding:0 20px; padding-bottom:30px;}
.wj_popUP_tit { /*border-bottom:1px solid #dce2e8;*/ padding-top:20px; height:26px; font-size:16px; color:#333; position:relative;}
.wj_popUP_tit:after { content:""; position:absolute; left:0; bottom:0; width:100%; height:1px; background:#dce2e8; }
.wj_popUP_tit_tools { position:absolute; right:0; top:20px;}
.wj_popUP_tit_tools a { margin-left:20px; color:#1e7bcd; cursor:pointer;}
.wj_popUP_tit span { font-size:12px; color:#777;}
.wj_popUP_tit span i { color:#f4343c;}
.wj_currentPrice { margin-top:15px; font-size:16px; color:#777;}
.wj_currentPrice span { font-size:16px; color:#f4343c;}
.wj_priceList { margin-top:10px;}
.wj_priceList li { float:left; margin-right:15px;}
.wj_priceList li:last-child { margin-right:0;}
.wj_popUPInput { border:1px solid #d1d9df; margin-top:5px; width:173px; height:31px; line-height:30px; color:#666;}
.wj_popUPInput input { margin-left:10px; width:130px; height:30px;}
.wj_popUPInput input:focus { border:none;}
.wj_popUP_btn { position:relative; margin-top:30px; text-align:right;}
.wj_btn { display:inline-block; margin-left:20px; width:100px; height:32px; line-height:32px; border-radius:3px; font-size:14px; text-align:center; cursor:pointer; }
.wj_btnSubmit { background:#1e7bcd; border:1px solid #1e7bcd; color:#fff;}
.wj_btnSubmit:hover { background:#1a6db6; border:1px solid #1a6db6; color:#fff;}
.wj_btnCancel { background:#fff; border:1px solid #1e7bcd; color:#1e7bcd;}
.wj_btnCancel:hover { color:#1a6db6; border:1px solid #1a6db6;}
.wj_checkbox { position:absolute; top:10px; left:0;}
.wj_checkbox input[type="checkbox"] { margin-right:10px; width:14px; height:14px; vertical-align:text-top;}
.wj_checkbox label { font-size:15px; vertical-align:middle;}
.wj_popUP_close { background:url(../images/wj_popClose_icon.png) no-repeat;cursor: pointer; width:50px; height:50px; position:absolute; right:-140px; top:-100px;}



