.search_gxt_title{ border-bottom: 1px solid #dce2e8; padding-bottom: 6px;}
.search_gxt_title span{height: 26px; display: inline-block; font-size: 16px; font-weight: bold; color: #333; text-align: left;}
.search_gxt_title span em{margin-bottom: 1px; display: inline-block; font-weight: bold; color: #1e7bcd; font-size: 16px;cursor: pointer;}
.search_gxt_title span em#no_data_titles{color: #f4343c;}
.search_gxt_title span em:hover{
	border-bottom: 1px solid #1e7bcd;
}
.gxt_dbox{ width: 100%; height: 640px; position: relative; margin: 0 auto;}
.gxt_time{ position: absolute; right: 0; top: 10px; z-index: 5;}
.gxt_time span{ display: block; width:160px; font-size:14px ; color: #999; text-align: right;}
.mao_length{ position: absolute; top: 20px; left: 0;}
.mao_length ul li{ width: 110px; margin-bottom: 10px;}
.mao_length ul li i{ width:13px; height: 13px; border-radius: 50%; display: inline-block; float: left;}
.mao_length ul li span{ font-size:14px ; color: #666;display: inline-block; padding-left: 10px; float: left; line-height: 13px;}
.mao_length ul li:nth-child(1) i{ background: #64a4f2;}
.mao_length ul li:nth-child(2) i{ background: #41ccdc;}
.mao_length ul li:nth-child(3) i{ background: #41dc8e;}
.mao_length ul li:nth-child(4) i{ background: #dcda41;}
.mao_length ul li:nth-child(5) i{ background: #feb535;}
.mao_length ul li:nth-child(6) i{ background: #f8926d;}
.mao_length ul li:nth-child(7) i{ background: #f36c77;}
.mao_length ul li:nth-child(8) i{ background: #d967dd;}
.mao_length ul li:nth-child(9) i{ background: #9675da;}
.mao-toolbar{ position: absolute; top: 40px; right: 0;}
.mao-toolbar ul li{ background: url(../images/fdsxqp.png) no-repeat; width:50px; cursor: pointer;}
.mao-toolbar ul li.fd{ background-position: 1px 0px; height: 40px;}
.mao-toolbar ul li.sx{ background-position: 1px -40px; height: 35px;}
.mao-toolbar ul li.qp{ background-position: 1px -75px; height: 40px;}
.mao-screen-area{width: 100%;height: 100%;background: #fff; position: relative;}
:-webkit-full-screen.mao-screen-area{width: 100%;height: 100% !important;background: #fff; }
:-moz-full-screen.mao-screen-area{width: 100%;height: 100% !important;background: #fff; }
:-ms-full-screen.mao-screen-area{width: 100%;height: 100% !important;background: #fff; }
:-o-full-screen.mao-screen-area{width: 100%;height: 100% !important;background: #fff; }
:full-screen.mao-screen-area{width: 100%;height: 100% !important;background: #fff; }

/* add by wangjing */
.wj_screenArea { position:fixed; left:0; top:0; width:100%; height:100%; z-index:1004; }
.wj_screenArea .mao_length { left:20px; }
.wj_screenArea .mao-toolbar { right:20px; }
.wj_screenArea .gxt_time { right:20px; }


