/**select box**/

.selec_box {
	width: 60%;
	float: left;
	font-size: 14px;
}
	@media only screen and (min-width: 600px) and (max-width: 1600px) {
		.edit_top ul li input{width: 50%;}
		.edit_top ul li .selec_box {width: 50%;}
	}
	
	@media only screen and (min-width: 1601px) and (max-width: 1921px) {
		.edit_top ul li input{width: 60%;}
		.edit_top ul li .selec_box {width: 60%;}
	}
.edit_top ul li .selec_box ul li {
	width: 100%;
	margin-top: 0;
}

.edit_top ul li button {
	display: inline-block;
	width: 80px;
	height: 30px;
	border-radius: 3px;
	border: 1px solid #ccc;
	margin-left: 15px;
	color: #333;
	background: #fff;
	font-size: 14px;
	float: left;
}

.edit_top ul li b {
	display: inline-block;
	font-style: normal;
	font-size: 14px;
	color: #333;
	max-width: 60%;
	float: left;
	height: 30px;
	line-height: 30px;
}

.edit_top ul li em {
	display: inline-block;
	position: relative;
	left: 0;
	top: 0;
	color: #6dc3ea;
	display: none;
	cursor: pointer;
}

.edit_top ul li.on input,
.edit_top ul li input:focus {
	border: 1px solid #6dc3ea;
	box-shadow: 0px 0px 1px #6dc3ea;
}

.edit_top ul li.on button {
	background: #6dc3ea;
	border: 1px solid #6dc3ea;
	color: #fff;
}

