}

.role_input span {
	display: inline-block;
	font-size: 14px;
	color: #333;
}

.role_input input {
	display: inline-block;
	font-size: 14px;
	color: #999;
	width: 310px;
	height: 40px;
	line-height: 40px;
	border: 1px solid #ccc;
	box-sizing: border-box;
	padding-left: 10px;
	margin-left: 15px;
}

.role_input input::-webkit-input-placeholder,
.role_input textarea::-webkit-input-placeholder {
	color: #000;
}

.role_input input:-moz-placeholder,
.role_input textarea:-moz-placeholder {
	color: #000;
}

.role_input input::-moz-placeholder,
.role_input textarea::-moz-placeholder {
	color: #000;
}

.role_input input:-ms-input-placeholder,
.role_input textarea:-ms-input-placeholder {
	color: #000;
}

