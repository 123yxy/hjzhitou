/* CSS Document */

.main_btn {
	width: 100%;
	margin: 0 auto;
	border-bottom: 1px solid #ccc;
	padding-bottom: 35px;
}
 
.btn_l a {
	display: inline-block;
	width: 180px;
	height: 50px;
	line-height: 50px;
	background: url(../images/icon/add.png) 33px center no-repeat;
	background-size: 23px 23px;
	background-color: #1fbba6;
	text-align: right;
	color: #fff;
	font-size: 18px;
	box-sizing: border-box;
	padding-right: 33px;
	
}

.btn_l a.addyg {
	background: url(../images/icon/add.png) 33px center no-repeat;
	background-size: 23px 23px;
	background-color: #1fbba6;
}

.btn_l a.edityg {
	background: url(../images/icon/edit.png) 33px center no-repeat;
	background-size: 23px 23px;
	background-color: #6dc3ea;
	margin-left: 15px;
}

.btn_l a.editqx {
	background: url(../images/icon/editqx.png) 33px 12px no-repeat;
	background-size: 30px 30px;
	background-color: #6dc3ea;
	margin-left: 15px;
}

.btn_r {
	margin-top: 30px;
}

.btn_r span {
	display: block;
	background: url(../images/icon/yg.png) left center no-repeat;
	background-size: 23px 23px;
	padding-left: 30px;
	color: #666;
	font-size: 14px;
}

.btn_r span em {
	display: inline-block;
	font-style: normal;
	font-size: 14px;
	color: #f86b4f;
}

.main_table {
	width: 100%;
	margin: 0 auto;
	margin-top: 30px;
}

.main_table table {
	border: none;
	border-collapse: collapse;
	width: 100%;
}

.main_table table tr {
	background: #fff none repeat scroll 0 0;
}

.main_table table tr:nth-child(1) {
	border-bottom: 1px solid #999;
}

.main_table table tr.hover {
	background: #e3f1f4;
}

.main_table table th {
	color: #333;
	font-size: 18px;
	font-weight: normal;
	height: 40px;
	line-height: 40px;
	text-align: center;
}

.main_table table td {
	border-bottom: 1px solid #e3e3e3;
	border-left: 1px dotted #e3e3e3;
	font-size: 12px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	word-break: break-all;
	word-wrap: break-word;
	color: #333;
}

.main_table table td:nth-child(1) {
	border-left: none;
}

.main_table table td a {
	color: #6dc3ea;
}

.main_table table td em {
	font-style: normal;
	font-size: 12px;
}

.main_table table td em.ykt {
	color: #1fbba6;
}

.main_table table td em.wkt {
	color: #fa8564;
}

.add_title {
	width: 100%;
	margin: 0 auto;
	margin-top: 20px;
}

.add_title span {
	display: block;
	text-align: left;
	height: 30px;
	line-height: 30px;
