	<script type="text/javascript">
		
		$(function(){
			//UTIL.nologin('gbbdqk');
			
		    var windowHeight = $(window).height()
		    $(".yuce-fx .container").height(windowHeight-195);
		    $(".contents").height(windowHeight - 290);
		    
		    //用户是否是付费的状态
			payOrNot();
			if(payOrNot()==true){//已付费的情况
				$(".noPay").hide();
				$(".yx-select").show();
			}else if(payOrNot()==false || payOrNot()==undefined){//未付费的时候
				$(".noPay").show();
				$(".yx-select").hide();
			}
			//点击深度查看
			deepLook();
		    
		    //点击tab切换
			$(".tab-list").delegate("span","click",function(){
				if($(this).hasClass("on")){
					return false;
				}else{
					$(this).addClass("on").siblings().removeClass("on");
					if($(this).text()=="图表"){
						$(".echarts-tb").show();
						$(".guben-gudong").hide();
						$(".yx-select").show();
						payOrNot();
						if(payOrNot()==true){//已付费的情况
							$(".noPay").hide();
						}else if(payOrNot()==false || payOrNot()==undefined){//未付费的时候
							$(".noPay").show();
						}
					}else{
						$(".noPay").hide();
						$(".echarts-tb").hide();
						$(".guben-gudong").show();
						$(".yx-select").hide();
					}
				}
			})
			findTWOHis();
			findStructureTable();
		})
	</script>

