.userMain-list { margin-top:30px; font-size:14px; position:relative;}
.userMain-list li { margin-bottom:15px; position:relative;}
.userMain-list label { display:inline-block; margin-right:30px; width:60px; text-align:right; font-weight:normal; color:#5c666e;}
.userMain-list label.error{/*nieshi 20170908*/
    
            color: #f4363e;
            /* right: -100px; */
            left: 450px ;
            position: absolute;
            top: 6px;
            width: 350px !important;
            text-align: left;
    }
.userMain-list input[type="text"] { border:1px solid #c8d1da; border-radius:5px; color:#3d464d; width:340px; height:36px; line-height:36px;}
.userMain-list input[type="text"]:focus { border:1px solid #1985e2;}
.userMain-list input[readonly],.userMain-list input[readonly]:focus { border:none;}
.userMenu { padding:25px 0;}
.userMenu li { font-size:16px; height:46px; line-height:46px;}
.userMenu li.active { background-color:#1985e2; }
.userMenu li.active a { color:#fff;}
.userMenu li.active a:hover { text-decoration:none;}
.userMenu li.active a.lishiyanbao em{background: url(../images/times.png)center top no-repeat;}
.userMenu li.active a[href]:hover { text-decoration:underline;}
.userMenu li.active a.myOrder em{background: url(../images/baise-dd.png)center top no-repeat;}
.userMenu li a { display:block; padding-left:60px; color:#3d464d;}
.userMenu li a em { display:inline-block; margin-right:25px; width:20px; height:20px; vertical-align:middle; background-image:url(../images/icon-user.png); background-repeat:no-repeat;}
.userFocus em { background-position:0 -25px;}
.userAccount em { background-position:0 -45px;}
.userChangePW em { background-position:0 -68px;}
.userMenu li a.myOrder em{background: url(../images/dingd.png)center top no-repeat;height: 24px;}
.userMenu li.active .userCenter em { background-position:-26px 0;}
.userMenu li.active .userFocus em { background-position:-26px -25px;}
.userMenu li.active .userAccount em { background-position:-26px -45px;}
.userMenu li.active .userChangePW em { background-position:-26px -68px;}
.userMain-editPhoto { position:absolute; top:0; right:0;}
.userMain-editPhoto .photo { background:#f3f6f9; border-radius:5px; width:286px; height:286px; overflow:hidden; position:relative;}
.userMain-editPhoto .photo img { width:100%; position:absolute; top:50%; left:50%; -webkit-transform:translate(-50%,-50%); -moz-transform:translate(-50%,-50%);}
.userMain-editPhoto .upload { margin-top:30px; text-align:center; position:relative;}
.userMain-editPhoto .upload input[type="file"] { margin-left:-63px; width:126px; height:32px; position:absolute; left:50%; top:0; opacity:0; cursor:pointer;}

.userFocus-group { margin-top:30px;}
.userFocus-checkbox li { display:inline-block;}
.userFocus-checkbox input { display:inline-block; width:160px; height:40px; position:relative; z-index:3; vertical-align:top; opacity:0; cursor:pointer;}
.userFocus-checkbox input + label { display:inline-block; background:#f3f6f9; border:1px solid #c8d1da; border-radius:5px; color:#5c666e; margin-left:-160px; margin-right:30px; margin-bottom:20px; width:160px; height:40px; line-height:40px; font-size:14px; font-weight:normal; text-align:center; vertical-align:top;}
.userFocus-checkbox input:checked + label { background:#1985e2; border:1px solid #1985e2; color:#fff;}
.userFocus-save { margin-top:30px; text-align:center;}

.userAccount-group .userMain-list li button { position:absolute; right:0; top:0;}

.userChangePW-group .userMain-list label { width:70px;}
