.userChangePW-group li .tips-error { position:absolute; left:356px; top:0; height:36px; line-height:36px; text-align:right; color:#f4363e;}
.userChangePW-group li .icon-error { position:absolute; top:11px; left:416px; display:block; background:url(../images/icon-common.png) no-repeat -49px 0; width:14px; height:14px;}
.userChangePW-group li .icon-success { position:absolute; top:11px; left:416px; display:block; background:url(../images/icon-common.png) no-repeat -34px 0; width:14px; height:14px;}

.loginFocus-title { padding-top:50px; margin-bottom:40px; text-align:center;}
.loginFocus-title:after { content:""; display:block; background:#dce2e8; margin:0 auto; margin-top:-15px; width:410px; height:1px;}
.loginFocus-title span { padding:0 15px; background:#fff; font-size:18px; font-weight:bold; color:#3d464d;}
.loginFocus-group { padding-bottom:40px;}
.loginFocus-group .userFocus-checkbox { padding-left:45px; min-height:300px;}
.loginFocus-group .userFocus-checkbox input + label { margin-right:20px;}
.loginFocus-button-group { margin:0 45px; border-top:1px solid #dce2e8;}
.loginFocus-tips { margin-top:10px; text-align:center; font-size:13px; color:#5c666e;}
.loginFocus-tips i { color:#f4363e;}
.loginFocus-button { margin-top:40px; text-align:center;}
.loginFocus-button button { padding:0 55px; height:40px;}

.loginFocus-group .userMain-list { margin:0 auto; width:430px;}
.loginFocus-group .userMain-list li:last-child { margin-top:40px;}
.loginFocus-group .userMain-list i { margin-right:5px; color:#f4363e;}
.loginFocus-group .userMain-list label { margin-right:15px;}
.loginFocus-group .userMain-list input[type="radio"] { display:inline-block; width:60px; height:36px; opacity:0; position:relative; z-index:5; cursor:pointer; vertical-align:top;}
.loginFocus-group .userMain-list input[type="radio"] + label { display:inline-block; border-radius:0 5px 5px 0; margin-left:-60px; margin-right:0; width:60px; height:36px; line-height:36px; text-align:center; background:#b9bcbf; color:#1985e2; vertical-align:top;}
.loginFocus-group .userMain-list input[type="radio"] + label:nth-of-type(2) { border-radius:5px 0 0 5px;}
.loginFocus-group .userMain-list input[type="radio"]:checked + label { background:#1985e2; color:#fff;}

.findPW-guiding { clear:both; overflow:hidden; margin:0 auto; margin-bottom:40px; padding-top:20px; width:700px; font-size:14px; color:#3d464d; }
.findPW-guiding:after { content:""; display:block; background:#b9bcbf; margin-top:50px; width:700px; height:1px;}
.findPW-guiding li { float:left; width:100px; text-align:center;}
.findPW-guiding li:not(:last-child) { margin-right:200px;}
.findPW-guiding span { display:block; background:#b9bcbf; border-radius:50%; margin-bottom:15px; width:100px; height:100px; text-align:center;}
.findPW-guiding li.active span { background:#1985e2;}
.findPW-guiding span em { display:inline-block; background-image:url(../images/icon-user.png); background-repeat:no-repeat;}
