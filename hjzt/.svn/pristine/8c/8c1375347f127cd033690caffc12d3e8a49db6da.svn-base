/*热搜*/
.hot-link {color:#fff; font-size:14px;min-height: 43px;position: relative;}
.hot-link .rs { font-weight:bold; margin-right:25px;display: block;float: left; margin-left:10px;}
.hot-link .rs li{cursor:pointer;}
.hot-link .rs .zbli{background-color: #fff;color:#aaa;}
.hot-link .zbsj{position: absolute;background-image:url('../images/xsj.png');width:14px;height: 8px;bottom: 3px;left:50px;}
.hot-link .ybsj{position: absolute;background-image:url('../images/xsj.png');width:14px;height: 8px;bottom: 3px;left:116px;display: none;}
.hot-link .rs li{float:left; margin-left:15px;display:block;height:34px;line-height: 34px;padding:0 10px;border-radius: 6px; }
.hot-links{display: block;float: left;margin-left: 60px;position: absolute;}
.hot-links span { color: rgba(255,255,255,.7); margin-right:14px;margin-bottom: 10px; display:inline-block; font-weight:normal;white-space : normal;padding: 1px 5px;border:1px solid #4b79c4;border-radius: 12px;font-size: 14px;box-sizing: border-box;cursor: pointer;}
.cover-logo { position:absolute; top:35px; left:10px;}
.cover-logo img { width:90px;}
.cover-logo img:first-child { width:60px;}
.index-searches{position: relative;}
.index-searches .sy-fdj{display: block;width: 24px;height: 24px;background: url(../images/sy-fdj.png)center center no-repeat;position: absolute;top: 12px;right: 20px;cursor: pointer;z-index: 2;}
.index-his-search{position: absolute;width: 100%;background-color: #fff;top: 46px;left: 0;z-index: 3;border: 1px solid #e2e1e6}
.index-his-search li{text-align: left;line-height: 26px;padding: 0 20px;}
.index-his-search li span{box-sizing: border-box;color: #777;display: inline-block;width: 100%;line-height: 26px;font-size: 14px;border-bottom: 1px solid #e2e1e6;cursor: pointer;}
.index-searches .index-his-search li:nth-child(1){border-bottom: 1px solid #e2e1e6;margin-top: 4px;padding: 0;}
.index-searches .index-his-search li:nth-last-child(1){margin-bottom: 4px;border-bottom: 0;}
.index-searches .index-his-search li:nth-child(1) span{font-weight: 700;font-size: 16px;color: #333;padding-left: 20px;}
.index-searches .index-his-search li:nth-child(1) span,.index-his-search li:nth-last-child(1) span{border-bottom: 0;}
.index-his-search .clear-icon{display: block;width: 20px;height: 20px;background: url(../images/yb-ljt.png)0px 0px no-repeat;position: absolute;top: 3px;right: 20px;cursor: pointer;}
.hot-link.new-links{margin-top: 20px;}
.qiehuan{margin-bottom: 12px;padding-left: 20px;box-sizing: border-box;}
.qiehuan span{font-size: 16px;display: inline-block;color: #fff;width: 76px;height: 26px;cursor: pointer;float: left;border-radius: 5px;text-align: center;line-height: 26px;}
.qiehuan span.on{background-color: #fff;color: #666;position: relative;}
.qiehuan span.on:after{display: block;content: "";width: 0px;height:0px;border-top: 8px solid #fff;border-bottom: 8px solid transparent;border-left: 8px solid transparent;border-right: 8px solid transparent;position: absolute;bottom: -14px;left: 50%;margin-left: -6px;}

.hot-searches span{color: rgba(255,255,255,.7);cursor: pointer;}
.rs-name1{margin-right: 10px;}
.hot-searches .rs-name2{margin-left: 10px;}
.hot-searches span.on,.hot-searches em.shuxian{color: #fff;}
.hot-searches span.notClick{color: #777;cursor: default;}
#hisData .swiper-slide.zjll1{width:375px;margin: 0 auto;}
#hisData .swiper-slide.zjll2{width:375px;margin-right: 60px;}
/*首页的背景动画*/
.index-bg{position: relative;}
.bg-wrap{position: absolute;width: 100%;height: 100%;top: 0;left: 0;}
.bg-wrap .bg{position: absolute;top: 0;left: 0;width: 100%;height: 100%; transition: 0.3s;}
.bg-wrap .bg-1{background-color: #f1f3f7;transform: translateX(-100%);}
.bg-wrap.active .bg-1{transform: translateX(0);transition: 0.75s;}
.bg-wrap .bg-2{background-color: #fff;transform: translateX(-100%);}
.bg-wrap.active .bg-2{transform: translateX(0);transition: 0.75s 0.5s;}

.top-yanbao{width: 100%;height: 160px;}
.top-yanbao span{display: block;width: 100%;height: 160px;background: url(../images/index-title.png);}



/*下面的小圆圈的跳动*/
.to-next-page {
    position: absolute;
    z-index: 20;
    bottom: 30px;
    left: 50%;
    margin-left: -24px;
    width: 48px;
    height: 48px;
    overflow: hidden;
    -webkit-animation: sdb05 1.5s infinite;
    animation: sdb05 1.5s infinite;
    -webkit-animation-duration: 2s;
    animation-duration: 2s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
    margin-top: 20px;
}
.to-next-page img{
	display: block;
	width: 100%;
	height: 100%;
}
@keyframes sdb05{
	from{
		bottom: 30px;
		opacity: 1;
	}
	to{
		bottom: 0px;
		opacity: 0;
	}
}


