.indexHot { margin-top:60px;}
h2 { margin-bottom:40px; text-align:center; font-size:32px; color:#666; font-weight:normal;}
h2:after { content:""; display:block; background:#1985e2; margin:0 auto; margin-top:15px; width:70px; height:2px;}

.index-cpjs { background:#fff; padding-bottom:100px;}
.index-cpjs h2 { padding-top:60px; margin-bottom:70px;}
.index-cpjs li { display:inline-block; width:22%; text-align:center; color:#666; font-size:14px;}
.index-cpjs li em { display:inline-block; background-image:url(../images/icon-index.png); background-repeat:no-repeat; width:78px; height:105px;}
.qmgc { background-position:0 5px;}
.sjjz { background-position:-110px 0;}
.yjhq { background-position:-220px 5px;}
.mfsy { background-position:-320px 5px;}
.index-cpjs li h3 { margin-bottom:15px; font-size:20px; font-weight:bold; color:#333;}
.index-cpjs li:not(:last-child) { margin-right:2.0%;}

.tx-group { background:#fff; margin:50px 0; padding:0 20px; padding-top:30px; padding-bottom:40px; position:relative;}
.tx-group h4 { margin-bottom:30px; font-size:22px; color:#333;}
.tx-group h4:after { content:""; display:block; background:#1985e2; margin-top:10px; width:54px; height:2px;}
.tx-link { position:absolute; top:30px; right:20px;}
.tx-link a { display:block; border-radius:30px; background:#1985e2; box-shadow:0 3px 5px 2px #c9e2f8; width:155px; height:44px; line-height:44px; font-size:16px; color:#fff; text-align:center;}
.tx-link a:hover { background:#1e7bcd; text-decoration:none;}
.yk-tx { float:left;}
.hy-tx { float:right;}
.tx { float:left; font-size:14px; color:#333;}
.tx:not(:last-child) { margin-right:15px;}
.tx span { display:block; background-image:url(../images/index-tq.png); background-repeat:no-repeat; margin-bottom:15px; height:173px;}
.tx01 { background:-15px 0;}
.tx02 { background:-300px 0;}
.tx03 { background:-590px 0;}
.tx04 { background:-880px 0;}

.cover-title { margin-top:105px; margin-left:90%; color:#000; font-size:16px; white-space:nowrap;}
.cover-title span { display:block; font-size:19px;}
.cover-link { position:absolute; right:22px; top:35px;}
.cover-link span { display:block; background:#1985e2; border-radius:5px; width:80px; height:27px; line-height:27px; text-align:center; color:#fff; font-size:13px;}
.cover-link span:hover{background-color: #1e7bcd;}
.icon-hot { position:absolute; right:0; top:0; width:46px; height:41px; z-index:99; background:url(../images/icon-index.png) no-repeat -237px -111px;}

.footerMenu { text-align:center;}
.footerMenu dl { display:inline-block; margin:40px 0; width:140px; vertical-align:top; color:#fff;}
.footerMenu dl:not(:last-child) { margin-right:70px;}
.footerMenu dt { font-size:20px; margin-bottom:10px;}
.footerMenu dd { font-size:14px; line-height:180%;}
.footerMenu dd a { color:#fff;}
.footerMenu dd img { display:block; margin-top:10px; width:128px; z-index:99;}
.footerMenu dl:not(:nth-child(2)) dt { margin-bottom:20px;}

