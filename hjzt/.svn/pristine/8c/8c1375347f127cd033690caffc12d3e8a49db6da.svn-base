.bg-blue { background:#1985e2;}
.bg-deepGray { background:#494949;}
.copyright { height:70px; line-height:70px; text-align:center; font-size:14px; color:#fff;}


.indexTop { width:100%; height:420px; background:#0a175b; /*background:#000 url(../images/indexTop-banner.jpg) no-repeat; background-size:100% 100%;*/ position:relative; overflow:hidden;}
.indexCanvas { position:absolute; top:0; left:0;}
.indexBg { position:absolute; top:0; left:0; z-index:9; opacity:.3;} 
.indexBg img { width:100%; height:100%;}
.indexTop .container { position:relative; z-index:13;}
.indexTop-main { padding-top:18px;}
.indexTop-main .userHead-logo{margin-top: -5px; padding-left:0;}
.indexTop-user { text-align:right; color:#fff; font-size:14px;float: right;max-width: 30%;}
.indexTop-user img { margin-right:15px; border-radius:50%;    width: 100%;
    height: 100%;}
/*.indexTop-form { background:url(../images/index-title.png)  center 90px no-repeat;}*/
.indexTop-input {padding-top:60px;text-align:center;}

.index-search{ width:758px; height:100px;margin: 0 auto;position: relative;}
/*.index-search:after{display: block;content:'';width: 17px;height: 17px;background: url(../images/index-search.png);position: absolute;top: 17px;right: 20px;}*/
.indexTop-input input { background:rgba(15,141,230,.2); border:1px solid rgb(15,141,230); border-radius:25px; padding:0 15px; width:758px; height:46px; font-size:14px; color:#fff;box-sizing:border-box; padding-left: 20px;}
.indexTop-input input::-webkit-input-placeholder { color:#fff;}
