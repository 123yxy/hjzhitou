/*遮罩层*/
.shade{
    width:100%;
    height:155px;
    position:absolute;
    top:0;
    left:0;
    background:rgba(255,255,255,.7);
    box-shadow: 0 0 8px #C3DAEE;
    text-align: center;
    padding:43px;
    box-sizing: border-box;
}
.shade>.prompt{
    font-size:22px;
    font-weight: bold;
    color:#232323;
    margin-bottom:20px;
}
.shade ul{
    width: 180px;
    margin: 0 auto;
}
.shade ul.active{
    width: 215px;
}
.shade li{
    float: left;
    padding: 6px 3px;
    border:1px solid #E4E4E4;
    border-radius: 3px;
    height:30px;
    box-sizing: border-box;
