news-url.hide{
    display: none;
}
.container,.fujianlist{
    width:1002px;
    margin:0 auto;
  /*  background: red;*/
}
.fujianlist a{
	color: #333;
	display: block;
	width: 100%;
	height: 30px;
	line-height: 30px;
	text-align: left;
}
.fujianlist h1{
	height: 40px;
	line-height:40px;
	font-size: 16px;
	font-weight: bold;
	text-align: left;
}
.fujianlist a:hover{
	color: #1985e2;
}
.main{
    margin-top:30px;
}
.detail{
    width:100%;

}
.detail_tit{
    color: #333333;
    font-size:18px;
    font-weight: bold;
}
.detail p{
    /*margin-bottom: 36px;*/
    font-size:14px;
    color: #555555;
    line-height: 24px;
}
.com_infor{
    /*height:40px;*/
    border-bottom: 1px solid #e4e4e4;
    padding-bottom:20px;
}
.com_infor p{
    font-size:12px;
    line-height:40px;
    color:#888888;
    float: left;
    margin-right:20px;
}
.detail_cont{
    line-height: 24px;
    margin-top:14px;
    font-size:14px;
    color:#555555;
  /*  height:20px!important;
    margin-bottom: 0!important;
    line-height: 20px!important;*/
}
.detail_pic{
    width: 868px;
    height:394px;
    margin:0 auto;
    margin-top:15px;
    margin-bottom: 30px;
}
.detail_pic img{
    width:100%;
    height: 100%;
}


.list_tit{
    width:100%;
    height:40px;
    background:url(../../images/Image/newdetail_bg.jpg);
    background-size:100% 100%;
    margin-bottom: 19px;
    
}
.list_tit p{
    color:#1985e2;
    font-size:16px;
    line-height:40px;
    margin-left:28px;
    font-weight: bold;
}

.list_cont{
    font-size:14px;
    line-height:25px;
    color:#555555;
    margin-bottom: 30px;

}
.list_pic{
    width: 868px;
    height:394px;
    margin:0 auto;
    margin-top:59px;
    margin-bottom: 20px;
}
.list_pic img{
    width: 100%;
    height: 100%;
}
.news-url a{
    color:#14a6ef;
}
