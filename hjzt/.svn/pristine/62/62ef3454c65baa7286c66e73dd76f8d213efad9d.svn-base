body { background:#f3f6f9;}
.none { display:none;}

.logHead { margin-top:40px; text-align:center;}
.logHead a{display: block;width: 200px;height: 68px;margin: 0 auto;}
.logHead a img{display: block;width: 100%;height: 100%;margin-left: 8px;}
.logFoot { /*margin-top:130px;*/ text-align:center; font-size:14px; color:#5c666e;}
.logFoot p { margin:5px 0;}
.titTab { font-size:18px; font-weight:bold; }
.titTab span:not(:last-child) { margin-right:40px;}
.titTab span { display:inline-block; color:#1e7bcd;}
.titTab span a { display:block; color:#3d464d;}
.titTab span a:hover { text-decoration:none;}
.titTab span a[href]:hover { text-decoration:underline;}
.titTab span.active:after { content:""; display:block; background:#1e7bcd; margin:0 auto; width:40px; height:2px;}
.logReg-main { margin-top:30px; text-align:center; font-size:14px;}
.logReg-main form { margin:0 auto; width:340px;}
.logReg-main ul { border:1px solid #c8d1da; background:#fff; border-radius:5px; margin-top:15px;}
.logReg-main li { padding-left: 14px; text-align:left; position:relative;box-sizing: border-box;}
.logReg-main li span,error { position:absolute; display:block; right:16px; top:0; line-height:36px;}/*shiqi right:15px*/
.logReg-main li span.none { display:none;}
.logReg-main li span.tips-error,label.error,span.error { color:#f4363e;right: 100px;
    position: absolute;
    top: 6px;font-weight:100}/*nieshi 20170907*/
.logReg-main li span.icon-success { background:url(../images/icon-common.png) no-repeat -34px 0; width:14px; height:14px; top:10px;}/*shiqi top:11px*/
.logReg-main li:not(:last-child) { border-bottom:1px solid #c8d1da;}
.logReg-main li input { border:none; height:36px; padding-left:0!important;}
.login-tools { margin-top:10px; font-size:12px; text-align:left; position:relative;color: #89949d;}
.checkbox-group input[type="checkbox"] { margin-left:20px; border:none; position:relative; z-index:3; opacity:0;}
.checkbox-group input[type="checkbox"]+label { display:inline-block; background:url(../images/icon-common.png) no-repeat; margin-left:-16px; margin-right:10px; width:16px; height:16px; vertical-align:middle; }
.checkbox-group input[type="checkbox"]:checked+label { background:url(../images/icon-common.png) no-repeat -17px 0;}
.checkbox-group label { font-weight:normal; vertical-align:middle; color:#5c666e; font-size:13px;}
.login-tools a { position:absolute; top:0; right:0; color:#1e7bcd;}
.loginReg-btn { margin-top:20px;}
.loginReg-btn button { background:#1985e2; border:none; border-radius:5px; width:100%; height:40px; color:#fff; font-size:14px;}
.login-others { margin-top:30px;}
.login-others-tit { font-size:14px; color:#5c666e;}
.login-others-tit:after { content:""; display:block; margin:0 auto; margin-top:-15px; width:224px; height:1px; background:#dce2e8;}
.login-others-links { margin-top:20px;}
.login-others-links a { display:inline-block; width:30px; height:26px; background-image:url(../images/icon-loginOthers.png); background-repeat:no-repeat;}
.login-others-links a:not(:last-child) { margin-right:50px;}
.login-others-links a.weixin { background-position:0 0;}
.login-others-links a.weixin:hover { background-position:0 -36px;}
.login-others-links a.qq { background-position:-40px 0;}
.login-others-links a.qq:hover { background-position:-40px -36px;}
.login-others-links a.weibo { background-position:-85px 0;}
.login-others-links a.weibo:hover { background-position:-85px -36px;}
.login-others-tit span { display:inline-block; background:#f3f6f9; width:104px; height:25px;}
.ewm { margin-top:40px;}
.ewm p:first-child { margin-bottom:20px; font-size:18px; color:#3d464d;}
.ewm p:last-child { margin-top:10px; color:#5c666e;}
.ewm p.saoma-img{width: 100%;height: 200px;padding:0 70px;box-sizing: border-box;}
.ewm p.saoma-img img{display: block;width: 100%;}

.reg-agreement { margin-top:10px;}
.reg-agreement a { color:#1e7bcd;}
.logReg-main li input.check-code { position:absolute; right:0; top:10px; background:#fff; border-left:1px solid #d7dde4; border-radius:0 5px 0 0; padding:0; width:100px; height:18px; line-height:18px; text-align:center;}
.logReg-main li.logReg-code span { right:110px;}

@media only screen and (min-width: 1367px) and (max-width: 1500px) {
	.ewm {margin-top: 20px;}
	.ewm p:first-child{margin-bottom: 10px;}
}
@media only screen and (min-width: 100px) and (max-width: 1366px) {
	.logReg-main{margin-top: 20px;}
	.ewm {margin-top: 20px;}
	.ewm p:first-child{margin-bottom: 10px;}
}