.ui-datepicker{
	width: 100%;
 	margin:0 auto;
	height: 100%;
	box-sizing: border-box;
	padding:0px;
}
.ui-datepicker .ui-datepicker-header{
	width: 100%;
	border:0px;
	height: 48px;
	background: #f0f7ff;
	
}
.ui-datepicker .ui-datepicker-prev, 
.ui-datepicker .ui-datepicker-next{
	margin-top: 10px;
}
.ui-state-hover.ui-datepicker-prev-hover,.ui-state-hover.ui-datepicker-next-hover{ border: none;}
.ui-datepicker .ui-datepicker-header .ui-datepicker-prev .ui-icon{
	width: 18px;
	height: 20px;
	/*margin-left: 10px;*/
	background: url(../images/index_icon.png)0px -881px no-repeat;
}
.ui-datepicker .ui-datepicker-header .ui-datepicker-next .ui-icon{
	left: 14px;
	width: 18px;
	height: 20px;
	background: url(../images/index_icon.png)-16px -881px no-repeat;
}
.ui-datepicker .ui-datepicker-title{
	margin:6px 30px 0px 30px;
}
.ui-datepicker .ui-datepicker-title select{
	width: 40%;
	height: 25px;
	line-height: 25px;
	/*border:1px solid #b7d6fa;*/
	margin:3px 0px;
	padding:0px 10px;
	box-sizing: border-box;
}
.ui-corner-all, .ui-corner-bottom, .ui-corner-left, .ui-corner-bl{
	border-left: 0;
	border-right: 0;
}
.ui-datepicker .ui-datepicker-title select:nth-child(1){
	margin-right: 10px;
}	
.ui-datepicker-title select option{
	box-sizing: border-box; padding-left: 14px; min-height: 2em !important;
}
.ui-datepicker .ui-datepicker-calendar td a{
	text-align: center;
	border:0px;
	background: #fff;
	border-radius: 8px;
	position: relative;	
	padding: 0.7em;
	font-family: arial;
	color: #494958;
}
.ui-datepicker .ui-datepicker-calendar td a.on{color: #fff;}
.ui-datepicker .ui-datepicker-calendar td a.on:after{
	content:'';
	display: block;
	width: 30px;
	height: 30px;
	background-color: red;
	position: absolute;
	top:2px;
	left: 50%;
	margin-left: -15px;
	border-radius: 15px;
}
.ui-datepicker .ui-datepicker-calendar td.active a{
    position: relative;
 }
.ui-datepicker .ui-datepicker-calendar td.active a:before{
      content:"";
      display: block;
      width: 20px;
      height: 3px;
      background-color: red;
      position: absolute;
      bottom: 0px;
      left: 0px;
    }
.datepicker .ui-datepicker .ui-datepicker-header {
    background-color: #f0f7ff !important;
}
.datepicker .ui-datepicker .ui-state-active {
    background-color: #f56d73;
    color: #fff !important;
}
.datepicker .ui-datepicker .ui-datepicker-calendar td {
    position: relative;
    padding: 0.7em;
}
.datepicker .ui-datepicker .ui-datepicker-calendar td a:hover {
    background-color: #f56d73 !important;
    color: #fff !important;
}
.datepicker .ui-datepicker .ui-datepicker-calendar td.highlight::before {
    background: #f56d73 none repeat scroll 0 0;
    border-radius: 50%;
    bottom: 4px;
    content: "";
    height: 4px;
    left: 50%;
    margin-left: -2px;
    position: absolute;
    width: 4px;
    z-index: 2;
}
.datepicker .ui-datepicker .ui-datepicker-prev{ left: 15px;}
.datepicker .ui-datepicker .ui-datepicker-next{ right: 15px;}
