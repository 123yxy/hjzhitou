<script>
	AsectionOne3();
	shiDaLiuTongGuDong();
	shiDaGuDong();
	gengXinshijian();
	gengXinshijianLt();
	$(function(){
		//findshareholders(null);
	    var windowHeight = $(window).height()
	    $(".yuce-fx .container").height(windowHeight-195);
    	$(".contents").height(windowHeight - 290);
    	//用户是否是付费的状态
		$("#timeList").on("click","li",function(){
			$(this).addClass("on").siblings().removeClass("on");
			var date=$(this).text();
			$("#reportData").html(date).removeClass("on");
			$("#timeList").slideUp();
			//findshareholders(date);
		})
		//十大股东选取时间
		$("#xzsdgd").on("click","li",function(){
			$(this).addClass("on").siblings().removeClass("on");
			var reportPeriod=$(this).text();
			$("#sdgdp").html(reportPeriod);
			$(this).parent().slideUp();
			shiDaGuDong(reportPeriod);
		})
		$("#sdgdp").on("click",function(){
			if($("#xzsdgd").css("display")=="block"){
				$("#xzsdgd").slideUp()
			}else{
				$("#xzsdgd").slideDown()
			}
		})

		//十大股东流通选取时间
		$("#sdltgd").on("click","li",function(){
			$(this).addClass("on").siblings().removeClass("on");
			var reportPeriod=$(this).text();
			$("#sdltgdp").html(reportPeriod);
			$(this).parent().slideUp();
			shiDaLiuTongGuDong(reportPeriod);
		})
		$("#sdltgdp").on("click",function(){
			if($("#sdltgd").css("display")=="block"){
				$("#sdltgd").slideUp()
			}else{
				$("#sdltgd").slideDown()
			}
		})
	})
	$(document).on("click",function(e){
			var e=e || window.event;
			var elem=e.target || e.srcElement;
			var target = $("#sdgd");
			while(elem!=null && elem.id!="sdgd"){
				elem = elem.parentElement;
				if(elem != target[0]){
					$("#xzsdgd").slideUp();
				}
			}
		})
	$(document).on("click",function(e){
			var e=e || window.event;
			var elem=e.target || e.srcElement;
			var target = $("#sdgdlt");
			while(elem!=null && elem.id!="sdgdlt"){
				elem = elem.parentElement;
				if(elem != target[0]){
					$("#sdltgd").slideUp();
				}
			}
		})
</script>
