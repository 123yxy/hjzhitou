/*閸樺娅嶪E閻ㄥ嫬绨抽柈銊︾泊閸斻劍娼�*/

html body {
	min-width: 1200px;
	background: #fafafa;
}


/*body{overflow:scroll;overflow-x: hidden;}*/

article,
aside,
figure,
footer,
header,
hgroup,
nav,
section {
	display: block;
}

html body * {
	outline: none;
}

ul {
	list-style: none;
}

li {
	list-style: none;
}

blockquote,
q {
	quotes: none;
}

blockquote:before,
blockquote:after,
q:before,
q:after {
	content: '';
	content: none;
}

a {
	margin: 0;
	padding: 0;
	font-size: 100%;
	vertical-align: baseline;
	background: transparent;
	text-decoration: none;
	color: #5c5c5c;
	font-size: 100%;
}

a {
	behavior: url(link.htc);
}

a:hober {
	text-decoration: none
}

a:focus {
	outline: 0;
}

del {
	text-decoration: line-through;
}

abbr[title],
dfn[title] {
	border-bottom: 1px dotted #000;
	cursor: help;
}

th {
	font-weight: bold;
	vertical-align: bottom;
}

td {
	font-weight: normal;
	vertical-align: top;
}

hr {
	display: block;
	height: 1px;
	border: 0;
	border-top: 1px solid #e3e4e8;
	padding: 0;
}

input {
	padding-top: 0;
	padding-bottom: 0;
}

input::-moz-focus-inner {
	border: none;
	padding: 0;
}

select,
input {
	vertical-align: middle;
}

select,
input,
textarea {
	font-size: 12px;
	margin: 0;
}

input[type="text"],
input[type="password"],
textarea {
	outline-style: none;
	-webkit-appearance: none;
	margin-right: 10px;
}

textarea {
	resize: none;
}

.fl {
	float: left;
}

.fr {
	float: right;
}

table {
	font-size: inherit;
	font: 100%;
	max-width: auto;
	border-collapse: collapse;
}

:focus {
	outline: none;
}

a:hover,
a:active {
	outline: none;
	text-decoration: none;
}

a:focus {
	text-decoration: none;
}

small {
	font-size: 85%;
}

strong,
th {
	font-weight: bold;
}

td,
td img {
	vertical-align: top;
}

pre,
code,
kbd,
samp {
	font-family: monospace, sans-serif;
}

button,
input,
select,
textarea {
	margin: 0;
}

button {
	width: auto;
	overflow: visible;
	border: none;
}

html {
	_background: url(about:blank) no-repeat fixed;
}

.clr:before,
.clr:after {
	display: block;
	height: 0;
	overflow: hidden;
	clear: both;
}

.clr:after {
	clear: both;
}

.clr {
	font-style: 0;
	height: 0;
	line-height: 0;
	overflow: hidden;
	clear: both;
	zoom: 1;
}

input[type="text"]:focus,
input[type="password"]:focus,
textarea:focus {
	border: 1px solid #5bc0de;
}

* {
	-webkit-tap-highlight-color: transparent;
	outline-color: transparent;
	padding: 0;
	margin: 0;
}


